{
  "name": "sphinx-myst-demo",
  "version": "1.0.0",
  "scripts": {
    "serve:xref": "serve ./myst -l 9090",
    "watch:xref": "npm-watch",
    "start:xref": "npm-run-all -p serve:xref watch:xref",
    "build:xref": "cross-env PYTHONPATH=. sphinx-build -b myst . myst",
    "start:myst": "myst start"
  },
  "watch": {
    "build:xref": {
      "patterns": [
        "**/*.{py,rst}"
      ],
      "runOnChangeOnly": false
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "serve": "^14.0.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.13.0",
    "mystmd": "^1.2.9"
  }
}
